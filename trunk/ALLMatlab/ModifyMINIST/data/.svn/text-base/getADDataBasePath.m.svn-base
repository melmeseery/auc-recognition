function y=getADDataBasePath(varargin)
%%%%%%
%getADDataBasePath 
%store the path of the database initial location 
%If one input, use white space delimiter
%
%%%%%%%%%%%%%%%%%%%%%
if (nargin == 1)
 [x] = varargin{1}(1); 
if (x==1)
 y='F:\AUC\Databases\MNIST';
else
    %(x==0)
        y='F:\AUC\Databases\Arabic Digits Databases';
end 
else
%y='F:\AUC\Databases\Arabic Digits Databases';
y='F:\AUC\Databases\Arabic Digits Databases';
%y='F:\AUC\Databases\ADD';
end 
